{
  "name": "fcoin-otc",
  "version": "0.0.0",
  "author": "imconfig <imconfig@aliyun.com>",
  "description": "FCoinOTC",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "npm version patch && node .electron-vue/build.js && electron-builder always",
    "build32": "npm version patch && node .electron-vue/build.js && electron-builder --ia32 always",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://xxxxxxxxxxxxx"
  },
  "build": {
    "productName": "bee",
    "appId": "org.imconfig.FCoinOTC",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "repo": "FCoinOTC",
        "private": false,
        "owner": "imconfig",
        "vPrefixedTagName": true,
        "protocol": "https",
        "releaseType": "release",
        "publishAutoUpdate": true
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns",
      "publish": [
        "github"
      ]
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "publish": [
        "github"
      ]
    },
    "linux": {
      "icon": "build/icons",
      "publish": [
        "github"
      ]
    }
  },
  "dependencies": {
    "@types/node-fetch": "^2.1.4",
    "@types/node-rsa": "^0.4.3",
    "@types/ws": "^6.0.1",
    "axios": "^0.18.0",
    "cryptico": "^1.0.2",
    "dateformat-util": "^1.0.5",
    "decimal.js": "^10.1.1",
    "electron-log": "^2.2.17",
    "electron-updater": "^4.0.6",
    "element-ui": "^2.4.11",
    "fcoin-nodejs-api": "^1.3.2",
    "http-proxy-agent": "^2.1.0",
    "https-proxy-agent": "^2.2.1",
    "mathjs": "^5.4.0",
    "md5-typescript": "^1.0.5",
    "nedb": "^1.8.0",
    "node-fetch": "^2.3.0",
    "node-rsa": "^1.0.2",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "vue": "^2.5.21",
    "vue-class-component": "^6.3.2",
    "vue-property-decorator": "^7.2.0",
    "vue-router": "^3.0.2",
    "vue-ts-loader": "0.0.3",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.0",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "@types/mathjs": "^4.4.4",
    "@types/nedb": "^1.8.6",
    "ajv": "^6.6.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.3.0",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^4.1.3",
    "electron-builder": "^20.38.3",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "0.4.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "ts-vue-loader": "^1.0.1",
    "tslint-config-mk": "^1.1.6",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.5"
  }
}
